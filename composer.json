{
  "name": "kak/panel",
  "description": "Yii2 panel widget",
  "type": "yii2-extension",
  "homepage": "https://github.com/sanchezzzhak/kak-panel",
  "keywords": [
    "panel bootstrap for yii2",
    "panel for yii2",
    "Yii2 widget",
    "Yii2 bootstrap panel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tutik Alexsandr",
      "email": "sanchezzzhak@ya.ru"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/jquery-slimscroll": "*",
    "yiisoft/yii2-bootstrap": "^2.0"
  },
  "require-dev": {
    "symfony/event-dispatcher-contracts": "2.2.0",
    "codeception/codeception": "4.1.22",
    "codeception/module-asserts": "1.3",
    "codeception/module-yii2": "1.1"
  },
  "autoload": {
    "psr-4": {
      "kak\\widgets\\panel\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
