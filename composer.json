{
  "name": "kak/panel",
  "description": "Yii2 panel widget",
  "type": "yii2-extension",
  "homepage": "https://github.com/sanchezzzhak/kak-panel",
  "keywords": [
    "panel bootstrap for yii2",
    "panel for yii2",
    "Yii2 widget",
    "Yii2 bootstrap panel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tutik Alexsandr",
      "email": "sanchezzzhak@ya.ru"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/jquery-slimscroll": "*",
    "yiisoft/yii2-bootstrap": "^2.0"
  },
  "require-dev": {
    "php": ">=7.1",
    "codeception/codeception": "^5.0.0 || 4.1.*",
    "phpunit/phpunit": "^9.5",
    "codeception/module-asserts": "^3.0 || ^1.1",
    "codeception/module-yii2": "^1.1.4",
    "codeception/module-filesystem": "^3.0 || ^1.0"
  },
  "autoload": {
    "psr-4": {
      "kak\\widgets\\panel\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
